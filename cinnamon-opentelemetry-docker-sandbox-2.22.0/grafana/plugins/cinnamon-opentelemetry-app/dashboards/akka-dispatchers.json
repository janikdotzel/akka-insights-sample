        {
          "datasource": "Cinnamon Prometheus (via OpenTelemetry)",
          "title": "Queue time ($Quantile quantile)",
          "description": "Statistics for how long tasks are in the queue. High queue times may be indicative of thread starvation issues.",
          "type": "timeseries",
          "id": 3,
          "interval": "10s",
          "fieldConfig": {
            "defaults": {
              "custom": {
                "drawStyle": "line",
                "lineInterpolation": "linear",
                "lineWidth": 2,
                "fillOpacity": 10,
                "gradientMode": "none",
                "showPoints": "never",
                "stacking": {
                  "mode": "none"
                },
                "axisPlacement": "auto",
                "axisLabel": null
              },
              "color": {
                "mode": "palette-classic"
              },
              "unit": "s",
              "min": 0,
              "max": null
            }
          },
          "options": {
            "legend": {
              "showLegend": false
            },
            "tooltip": {
              "mode": "multi",
              "sort": "desc"
            }
          },
          "span": 6,
          "targets": [
            {
              "expr": "histogram_quantile($Quantile, {__name__=~\"akka_dispatcher_queue_time_seconds(|_bucket)\", host_name=~\"$Servers\", service_name=~\"$Applications\", actor_system=~\"$ActorSystems\", dispatcher=~\"$Dispatchers\"})",
              "format": "time_series",
              "legendFormat": "{{host_name}} / {{actor_system}} / {{dispatcher}}"
            }
          ]
        },
