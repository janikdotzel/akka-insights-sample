            "tooltip": {
              "mode": "multi",
              "sort": "desc"
            }
          },
          "span": 6,
          "targets": [
            {
              "expr": "akka_recovery_permitter_pending_entities_entity_bucket{host_name=~\"$Servers\", service_name=~\"$Applications\", actor_system=~\"$ActorSystems\"}",
              "format": "time_series",
              "legendFormat": "{{service_name}} / {{host_name}} / {{actor_system}} / $Quantile"
