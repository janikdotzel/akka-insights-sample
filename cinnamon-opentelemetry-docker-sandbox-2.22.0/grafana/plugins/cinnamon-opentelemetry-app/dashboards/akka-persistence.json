      "height": "250px",
      "panels": [
        {
          "datasource": "Cinnamon Prometheus (via OpenTelemetry)",
          "title": "Used permits ($MetricType)",
          "description": "Number of used permits. A permit system is used to place a maximum limit on the number of concurrent recoveries that can be in progress at the same time in order to not overload the system and the data store. The limit is controlled via configuration akka.persistence.max-concurrent-recoveries.",
          "type": "timeseries",
          "id": 3,
          "interval": "10s",
          "fieldConfig": {
            "defaults": {
              "custom": {
                "drawStyle": "line",
                "lineInterpolation": "linear",
                "lineWidth": 2,
                "fillOpacity": 10,
                "gradientMode": "none",
                "showPoints": "never",
                "stacking": {
                  "mode": "none"
                },
                "axisPlacement": "auto",
                "axisLabel": null
              },
              "color": {
                "mode": "palette-classic"
              },
              "unit": "short",
              "min": 0,
              "max": null
            }
          },
          "options": {
            "legend": {
              "showLegend": false
            },
            "tooltip": {
              "mode": "multi",
              "sort": "desc"
            }
          },
          "span": 6,
          "targets": [
            {
              "expr": "histogram_quantile($Quantile, {__name__=~\"akka_recovery_permitter_used_permits_permit(|_bucket)\", host_name=~\"$Servers\", service_name=~\"$Applications\", actor_system=~\"$ActorSystems\"})",
              "format": "time_series",
              "legendFormat": "{{service_name}} / {{host_name}} / {{actor_system}} / $Quantile"
            },
            {
              "expr": "akka_recovery_permitter_max_permits_permit{host_name=~\"$Servers\", service_name=~\"$Applications\"}",
              "format": "time_series",
              "legendFormat": "{{service_name}} / {{host_name}} / {{actor_system}} / max-limit"
            }
          ]
        },
        {
          "datasource": "Cinnamon Prometheus (via OpenTelemetry)",
          "title": "Pending permits ($MetricType)",
