      "height": "250px",
      "panels": [
        {
          "datasource": "Cinnamon Prometheus (via OpenTelemetry)",
          "title": "Remote Messages Sent: 1 min",
          "description": "",
          "type": "timeseries",
          "id": 1,
          "interval": "10s",
          "fieldConfig": {
            "defaults": {
              "custom": {
                "drawStyle": "line",
                "lineInterpolation": "linear",
                "lineWidth": 2,
                "fillOpacity": 10,
                "gradientMode": "none",
                "showPoints": "never",
                "stacking": {
                  "mode": "none"
                },
                "axisPlacement": "auto",
                "axisLabel": "messages/second"
              },
              "color": {
                "mode": "palette-classic"
              },
              "unit": "ops",
              "min": 0,
              "max": null
            }
          },
          "options": {
            "legend": {
              "showLegend": false
            },
            "tooltip": {
              "mode": "multi",
              "sort": "desc"
            }
          },
          "span": 6,
          "targets": [
            {
              "expr": "irate(akka_message_remote_sent_messages_msg_total{host_name=~\"$Servers\", service_name=~\"$Applications\", actor_system=~\"$ActorSystems\", dispatcher=~\"$Dispatchers\", actor=~\"$RemoteActors\", message=~\"$Messages\"}[1m])",
              "format": "time_series",
              "legendFormat": "{{host_name}} / {{dispatcher}} / {{actor}} / {{message}}"
            }
          ]
        },
