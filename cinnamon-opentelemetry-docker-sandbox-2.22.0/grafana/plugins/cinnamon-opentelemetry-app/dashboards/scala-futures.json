{
  "id": null,
  "title": "Scala Futures",
  "tags": [],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "hideControls": true,
  "sharedCrosshair": false,
  "rows": [
    {
      "title": "",
      "collapse": false,
      "editable": true,
      "height": "250px",
      "panels": [
        {
          "datasource": "Cinnamon Prometheus (via OpenTelemetry)",
          "title": "Created futures: 1 min rate",
          "description": "The rate of Futures being created (for the given name).",
          "type": "timeseries",
          "id": 4,
          "interval": "10s",
          "fieldConfig": {
            "defaults": {
              "custom": {
                "drawStyle": "line",
                "lineInterpolation": "linear",
                "lineWidth": 2,
                "fillOpacity": 10,
                "gradientMode": "none",
                "showPoints": "never",
                "stacking": {
                  "mode": "none"
                },
                "axisPlacement": "auto",
                "axisLabel": "futures/second"
              },
              "color": {
                "mode": "palette-classic"
              },
              "unit": "ops",
              "min": 0,
              "max": null
            }
          },
          "options": {
            "legend": {
              "showLegend": false
            },
            "tooltip": {
              "mode": "multi",
              "sort": "desc"
            }
          },
          "span": 6,
          "targets": [
            {
              "expr": "irate(scala_future_created_futures_total{host_name=~\"$Servers\", service_name=~\"$Applications\", scala_future=~\"$Futures\"}[1m])",
              "format": "time_series",
              "legendFormat": "{{service_name}} / {{scala_future}}"
            }
          ]
        },
        {
          "datasource": "Cinnamon Prometheus (via OpenTelemetry)",
          "title": "Future completing time ($Quantile quantile)",
          "description": "The time from creation to completing the underlying Promise for this Future.",
          "type": "timeseries",
          "id": 3,
          "interval": "10s",
          "fieldConfig": {
            "defaults": {
              "custom": {
                "drawStyle": "line",
                "lineInterpolation": "linear",
                "lineWidth": 2,
                "fillOpacity": 10,
                "gradientMode": "none",
                "showPoints": "never",
                "stacking": {
                  "mode": "none"
                },
                "axisPlacement": "auto",
                "axisLabel": null
              },
              "color": {
                "mode": "palette-classic"
              },
              "unit": "s",
              "min": 0,
              "max": null
            }
          },
          "options": {
            "legend": {
              "showLegend": false
            },
            "tooltip": {
              "mode": "multi",
              "sort": "desc"
            }
          },
          "span": 6,
          "targets": [
            {
              "expr": "histogram_quantile($Quantile, {__name__=~\"scala_future_completing_time_seconds(|_bucket)\", host_name=~\"$Servers\", service_name=~\"$Applications\", scala_future=~\"$Futures\"})",
              "format": "time_series",
              "legendFormat": "{{service_name}} / {{scala_future}}"
            }
          ]
        }
      ]
    },
