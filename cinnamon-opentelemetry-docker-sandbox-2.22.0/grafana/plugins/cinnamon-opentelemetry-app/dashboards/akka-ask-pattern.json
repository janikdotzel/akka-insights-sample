{
  "id": null,
  "title": "Akka Ask Pattern",
  "tags": [],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "hideControls": true,
  "sharedCrosshair": false,
  "rows": [
    {
      "title": "",
      "collapse": false,
      "editable": true,
      "height": "250px",
      "panels": [
        {
          "datasource": "Cinnamon Prometheus (via OpenTelemetry)",
          "title": "Asked requests: 1min rate",
          "description": "The number of messages that actors have been asked in the selected time frame.",
          "type": "timeseries",
          "id": 2,
          "interval": "10s",
          "fieldConfig": {
            "defaults": {
              "custom": {
                "drawStyle": "line",
                "lineInterpolation": "linear",
                "lineWidth": 2,
                "fillOpacity": 10,
                "gradientMode": "none",
                "showPoints": "never",
                "stacking": {
                  "mode": "none"
                },
                "axisPlacement": "auto",
                "axisLabel": "messages/second"
              },
              "color": {
                "mode": "palette-classic"
              },
              "unit": "ops",
              "min": 0,
              "max": null
            }
          },
          "options": {
            "legend": {
              "showLegend": false
            },
            "tooltip": {
              "mode": "multi",
              "sort": "desc"
            }
          },
          "span": 6,
          "targets": [
            {
              "expr": "irate(akka_message_asked_requests_msg_total{host_name=~\"$Servers\", service_name=~\"$Applications\", actor_system=~\"$ActorSystems\", actor=~\"$Actors\", message=~\"$Messages\"}[1m])",
              "format": "time_series",
              "legendFormat": "{{host_name}} / {{actor}} / {{message}}"
            }
          ]
        },
        {
          "datasource": "Cinnamon Prometheus (via OpenTelemetry)",
          "title": "Failed requests: 1min rate",
          "description": "The number of failure responses received in the selected time frame.",
          "type": "timeseries",
          "id": 5,
          "interval": "10s",
          "fieldConfig": {
            "defaults": {
              "custom": {
                "drawStyle": "line",
                "lineInterpolation": "linear",
                "lineWidth": 2,
                "fillOpacity": 10,
                "gradientMode": "none",
                "showPoints": "never",
                "stacking": {
                  "mode": "none"
                },
                "axisPlacement": "auto",
                "axisLabel": "messages/second"
              },
              "color": {
                "mode": "palette-classic"
              },
              "unit": "ops",
              "min": 0,
              "max": null
            }
          },
          "options": {
            "legend": {
              "showLegend": false
            },
            "tooltip": {
              "mode": "multi",
              "sort": "desc"
            }
          },
          "span": 6,
          "targets": [
            {
              "expr": "irate(akka_actor_failed_request_event{host_name=~\"$Servers\", service_name=~\"$Applications\", actor_system=~\"$ActorSystems\", actor=~\"$Actors\"}[1m])",
              "format": "time_series",
              "legendFormat": "{{host_name}} / {{actor}}"
            }
          ]
        },
        {
          "datasource": "Cinnamon Prometheus (via OpenTelemetry)",
          "title": "Expired requests",
          "description": "The number of messages that haven't been answered before a specified timeout in the selected time frame.",
          "type": "timeseries",
          "id": 4,
          "interval": "10s",
          "fieldConfig": {
            "defaults": {
              "custom": {
                "drawStyle": "line",
                "lineInterpolation": "linear",
                "lineWidth": 2,
                "fillOpacity": 10,
                "gradientMode": "none",
                "showPoints": "never",
                "stacking": {
                  "mode": "none"
                },
                "axisPlacement": "auto",
                "axisLabel": null
              },
              "color": {
                "mode": "palette-classic"
              },
              "unit": "none",
              "min": 0,
              "max": null
            }
          },
          "options": {
            "legend": {
              "showLegend": false
            },
            "tooltip": {
              "mode": "multi",
              "sort": "desc"
            }
          },
          "span": 6,
          "targets": [
            {
              "expr": "akka_message_expired_requests_msg_total{host_name=~\"$Servers\", service_name=~\"$Applications\", actor_system=~\"$ActorSystems\", actor=~\"$Actors\", message=~\"$Messages\"}",
              "format": "time_series",
              "legendFormat": "{{host_name}} / {{actor}} / {{message}}"
            }
          ]
        },
        {
          "datasource": "Cinnamon Prometheus (via OpenTelemetry)",
          "title": "Expired requests (by timeout)",
          "description": "The number of requests that timed out in the selected time frame grouped by specified timeout values.",
          "type": "timeseries",
          "id": 1,
          "interval": "10s",
          "fieldConfig": {
            "defaults": {
              "custom": {
                "drawStyle": "line",
                "lineInterpolation": "linear",
                "lineWidth": 2,
                "fillOpacity": 10,
                "gradientMode": "none",
                "showPoints": "never",
                "stacking": {
                  "mode": "none"
                },
                "axisPlacement": "auto",
                "axisLabel": null
              },
              "color": {
                "mode": "palette-classic"
              },
              "unit": "none",
              "min": 0,
              "max": null
            }
          },
          "options": {
            "legend": {
              "showLegend": false
            },
            "tooltip": {
              "mode": "multi",
              "sort": "desc"
            }
          },
          "span": 6,
          "targets": [
            {
              "expr": "akka_timeout_expired_requests_msg_total{host_name=~\"$Servers\", service_name=~\"$Applications\", actor_system=~\"$ActorSystems\", actor=~\"$Actors\", timeout=~\"$Timeouts\"}",
              "format": "time_series",
              "legendFormat": "{{host_name}} / {{actor}} / {{timeout}}"
            }
          ]
        },
        {
          "datasource": "Cinnamon Prometheus (via OpenTelemetry)",
          "title": "Success response time ($Quantile quantile)",
          "description": "The time from the moment the message was sent till the moment when the answer was received and before the specified timeout, counting only successful responses.",
          "type": "timeseries",
          "id": 3,
          "interval": "10s",
          "fieldConfig": {
            "defaults": {
              "custom": {
                "drawStyle": "line",
                "lineInterpolation": "linear",
                "lineWidth": 2,
                "fillOpacity": 10,
                "gradientMode": "none",
                "showPoints": "never",
                "stacking": {
                  "mode": "none"
                },
                "axisPlacement": "auto",
                "axisLabel": null
              },
              "color": {
                "mode": "palette-classic"
              },
              "unit": "s",
              "min": 0,
              "max": null
            }
          },
          "options": {
            "legend": {
              "showLegend": false
            },
            "tooltip": {
              "mode": "multi",
              "sort": "desc"
            }
          },
          "span": 6,
          "targets": [
            {
              "expr": "histogram_quantile($Quantile, {__name__=~\"akka_message_success_response_time_seconds(|_bucket)\", host_name=~\"$Servers\", service_name=~\"$Applications\", actor_system=~\"$ActorSystems\", actor=~\"$Actors\"})",
              "format": "time_series",
              "legendFormat": "{{host_name}} / {{actor}} / {{message}}"
            }
          ]
        },
        {
          "datasource": "Cinnamon Prometheus (via OpenTelemetry)",
          "title": "Late response time ($Quantile quantile)",
          "description": "The time from the moment the message was sent till the moment when the answer was received, counting only responses received after timeout.",
          "type": "timeseries",
          "id": 6,
          "interval": "10s",
          "fieldConfig": {
            "defaults": {
              "custom": {
                "drawStyle": "line",
                "lineInterpolation": "linear",
                "lineWidth": 2,
                "fillOpacity": 10,
                "gradientMode": "none",
                "showPoints": "never",
                "stacking": {
                  "mode": "none"
                },
                "axisPlacement": "auto",
                "axisLabel": null
              },
              "color": {
                "mode": "palette-classic"
              },
              "unit": "s",
              "min": 0,
              "max": null
            }
          },
          "options": {
            "legend": {
              "showLegend": false
            },
            "tooltip": {
              "mode": "multi",
              "sort": "desc"
            }
          },
          "span": 6,
          "targets": [
            {
              "expr": "histogram_quantile($Quantile, {__name__=~\"akka_message_late_response_time_seconds(|_bucket)\", host_name=~\"$Servers\", service_name=~\"$Applications\", actor_system=~\"$ActorSystems\", actor=~\"$Actors\"})",
              "format": "time_series",
              "legendFormat": "{{host_name}} / {{actor}} / {{message}}"
            }
          ]
        }
      ],
      "repeat": "Applications",
      "title": "$Applications"
    }
  ],
